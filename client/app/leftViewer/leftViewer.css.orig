}

.contact {
    word-wrap: break-word;
    font-size: 12px;
    width: 1em;
    z-index: 20;
}

#contactContainer {
    bottom: 0;
    position: absolute !important;
    width: 40px;
    padding-top: 20px;
    padding-bottom: 50px; 
    background-color: white;
}

/*Animation on Mail*/
#mailContainer {
    top: 0;
    margin-left: 40px;
    border-left: 1px solid #b8e1e1;
    width: 350px;
    height: 199px;
    background-color: white;
    position: absolute;
}

/*Left Panel*/
#leftPanel {
    background-color: brown;
    opacity: 0.75;
    width: 90%;
    height: 200px;
    z-index: 9999;
}

/*Unfold A PROPOS*/
#aProposTexte {
    height: calc(100% - 100px);
    display: block;
    width: calc(100% - 80px);
    padding-left: 80px;
    margin-top: 100px;
    position: absolute;
    top: 0px;
    overflow: hidden;
}

.fold #aProposTexte {
    visibility: hidden;
    opacity: 0;
    transition: visibility 0.5s, opacity 0.5s ease;
    transition: opacity 1s ease;
}

.unfold #aProposTexte {
    visibility: visible;
    opacity: 1;
    transition: visibility 0.5s, opacity 0.5s ease;
    transition: opacity 1s ease;
}















