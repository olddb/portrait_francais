//Lazy Loading gallery images

(function() {
var w = window,
    d = document;
    function handleGallerieResponsive() {
	    var e = d.documentElement,
	    g = d.getElementsByTagName('body')[0],
	    x = w.innerWidth || e.clientWidth || g.clientWidth,
	    y = w.innerHeight|| e.clientHeight|| g.clientHeight;
		if (x >= 1300) {
			$('.twoColumn img, .oneColumn img').removeClass('b-lazy').attr("src", function() {
			    return $(this).attr('data-src');
			});
		}
		else if (x >= 800) {
			$('.threeColumn img, .oneColumn img').removeClass('b-lazy').attr("src", function() {
			    return $(this).attr('data-src');
			});
		}
		else {
			$('.twoColumn img, .threeColumn img').removeClass('b-lazy').attr("src", function() {
			    return $(this).attr('data-src');
			});
		}
	}
	$( window ).resize(function() {
		handleGallerieResponsive();
	});
	var bLazy = new Blazy({
		container: '#galleryContainer'
	});
})();
         
</script>

